Main()
{
    a := 2;
    b := 3;
    c := &a;
    Assert(*c == a);
    c = &b;
    Assert(*c == b);
    d := {"key" : 1};
    e := &d;
    Assert((*e).key == 1);
    x := Main;
    xx := &Main;
    Assert(x == xx);
    Assert(Main == &Main);
    array := [3, 4, 5, 6];
    Assert(*&array[2] == 5);
    string := "asdf";
    char := &string[0];
    *char = "F";
    Assert(string == "Fsdf");
    *char = "ZZZZ"; # Overwrites storage char by char - caps out.
    Assert(string == "ZZZZ");
    ret 0;
}
