Main()
{
    Assert(Len(String("{.1}" % [1.2345])) == 3);
    Assert(Len(String("{.2}" % [1.2345])) == 4);
    Assert(Len(String("{.3}" % [1.2345])) == 5);
    Assert(Len(String("{.4}" % [1.2345])) == 6);
    Assert(Len(String("{0.1}" % [1.2345])) == 3);
    Assert(Len(String("{0.2}" % [1.2345])) == 4);
    Assert(Len(String("{0.3}" % [1.2345])) == 5);
    Assert(Len(String("{0.4}" % [1.2345])) == 6);
    Assert(Len(String("{10.1}" % [1.2345])) == 10);
    Assert(Len(String("{10.2}" % [1.2345])) == 10);
    Assert(Len(String("{10.3}" % [1.2345])) == 10);
    Assert(Len(String("{10.4}" % [1.2345])) == 10);
    Assert(Len(String("{10.4}" % [1.0123456789])) == 10);
    ret 0;
}
