Main()
{
    a := 0;
    b := 0;
    c := 0;
    d := 0;
    B := 10;
    A := 99;
    while(a < B)
    {
        a += 1;
        while(c < A)
        {
            c += 1;
            continue;
            d += 1;
        }
        continue;
        b += 1;
    }
    Assert(d == 0);
    Assert(c == A);
    Assert(b == 0);
    Assert(a == B);
    ret 0;
}
