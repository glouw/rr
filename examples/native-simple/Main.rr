lib Math
{
    Math_Add(self, other);
    Math_Sub(self, other);
    Math_Div(self, other);
    Math_Mul(self, other);
}

Main()
{
    a := 1;
    b := 1;
    c := 1;
    d := 1;
    other := 2;
    Math_Add(a, other);
    Math_Sub(b, other);
    Math_Div(c, other);
    Math_Mul(d, other);
    Assert(a == 3);
    Assert(b == -1);
    Assert(c == 0.5);
    Assert(d == 2);
    ret 0;
}
