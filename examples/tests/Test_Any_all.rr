Main()
{
    Assert(All([true, true, true]) == true);
    Assert(All([true, true, false, true]) == false);
    Assert(All([true, true, 0, true]) == false);
    Assert(All([]) == true);
    Assert(Any([false, true, false]) == true);
    Assert(Any([0, true, false]) == true);
    Assert(Any([0, true, 0]) == true);
    Assert(Any([true]) == true);
    Assert(Any([]) == false);
    ret 0;
}
